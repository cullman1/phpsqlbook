RewriteEngine On    # Turn on the rewriting engine
ErrorDocument 404  /404.php
RewriteRule ^/?$     category.php        [NC,L]
RewriteRule ^admin/?$   admin/article.php      [NC,L]
RewriteRule ^login/?$   users/login.php      [NC,L]
RewriteRule ^register/?$  users/register.php      [NC,L]
RewriteRule ^logout/?$   users/logout.php      [NC,L]
RewriteRule ^like/?$   users/like.php      [QSA,L]
RewriteRule ^unlike/?$   users/like.php     [QSA,L]
RewriteRule ^add_comment/?$ users/add_comment.php?id=$1   [QSA,L]
RewriteRule ^add_comment_reply/?$ users/add_comment.php?id=$1   [QSA,L]
RewriteRule ^about/?$    article.php?title=about    [NC,L]
RewriteRule ^contact/?$  article.php?title=contact    [NC,L]
RewriteRule ^search/?$ search.php       [QSA,NC,L]
RewriteRule ^home/?$ index.php       [QSA,NC,L]
RewriteRule ^profile/?$ users/user.php?id=$1       [QSA,NC,L]
RewriteRule ^author/([^/.]+)/?$  author.php?name=$1&show=$2&term=$3&to=$4 [QSA,L]
RewriteRule ^[A-z0-9\-]+/([A-z0-9\-]+)/?$   article.php?title=$1     [NC,L]